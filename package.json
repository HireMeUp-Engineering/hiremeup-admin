{
  "name": "hireme-admin",
  "version": "0.1.0",
  "private": true,
  "homepage": "/admin",
  "dependencies": {
    "@mui/icons-material": "^7.3.6",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.126",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "date-fns": "^4.1.0",
    "jwt-decode": "^4.0.0",
    "ra-data-simple-rest": "^5.13.4",
    "react": "^19.2.3",
    "react-admin": "^5.13.4",
    "react-dom": "^19.2.3",
    "react-scripts": "5.0.1",
    "recharts": "^3.6.0",
    "serve": "^14.2.4",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "serve": "serve -s build -l 3000",
    "pm2:start": "pm2 start pm2.config.js --env production",
    "pm2:start:dev": "pm2 start pm2.config.js --env development",
    "pm2:reload": "pm2 reload pm2.config.js --env production --update-env",
    "pm2:stop": "pm2 stop hireme-admin",
    "pm2:restart": "pm2 restart hireme-admin",
    "pm2:delete": "pm2 delete hireme-admin",
    "pm2:status": "pm2 status",
    "pm2:logs": "pm2 logs hireme-admin",
    "pm2:monit": "pm2 monit",
    "pm2:save": "pm2 save",
    "deploy": "npm run build && npm run pm2:reload",
    "deploy:fresh": "npm install && npm run build && npm run pm2:start"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
